---
import TodoVue from '../components/Todo.vue';
import TodoSvelte from '../components/Todo.svelte';
import TodoReact from '../components/Todo.jsx';
// import Footer from '../components/Footer.astro';
import Main from '../layouts/main.astro';
---
<Main />
<section class="contentTodos">
  <div class="todo">
    <h1>React</h1>
    <TodoReact:load />
  </div>
  <div class="todo">
    <h1>Svelte</h1>
    <TodoSvelte:load />
  </div>
  <div class="todo">
    <h1>Vue</h1>
    <TodoVue:load />
  </div>
</section>
